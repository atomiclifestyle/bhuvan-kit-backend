{
    "name": "bhuvan-backend",
    "version": "1.0.0",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "dev": "node index.js",
        "build": "tsc",
        "start": "node index.js",
        "local": "docker-compose -f src/docker/docker-compose.yml up -d && npx wait-on tcp:5432 && nodemon index.js",
        "stop": "docker-compose -f src/docker/docker-compose.yml down"
    },
    "dependencies": {
        "axios": "^1.6.8",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^4.18.2",
        "https-proxy-agent": "^7.0.6",
        "pg": "^8.16.3",
        "sequelize": "^6.37.7"
    },
    "devDependencies": {
        "nodemon": "^3.1.10",
        "wait-on": "^8.0.3"
    }
}
